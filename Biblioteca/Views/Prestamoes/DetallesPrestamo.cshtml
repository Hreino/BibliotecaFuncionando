@model ICollection<Biblioteca.Models.detallePrestamo>
<!--Importando el modelo de detalle prestamo-->
<div class="row">

    <div class="col-md-6">
        <table class="table table-bordered table-hover table-responsive">
            <thead>
                <tr class="info">
                    <th class="text-center">
                        Cantidad
                    </th>
                    <th class="text-center">
                        Libro
                    </th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <tr class="text-center">
                            <td>@item.cantidad</td>
                            <td>@item.Libro.Titulo</td>
                        </tr>
                    }
                }
            </tbody>

        </table>


    </div>

</div>




